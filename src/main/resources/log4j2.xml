<?xml version="1.0" encoding="UTF-8"?>
<configuration  status="WARN">  
  <appenders>  
    <Console name="Console" target="SYSTEM_OUT">  
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} [%logger{36}:%t] %-5level:%msg%n"/>
    </Console>  
  </appenders>  
  <loggers>  
    <root level="debug">  
      <appender-ref ref="Console"/>  
    </root>  
  </loggers>  
</configuration>  
